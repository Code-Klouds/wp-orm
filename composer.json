{
  "name": "code-klouds/wp-orm",
  "description": "Wordpress ORM with Eloquent and Phinx",
  "type": "package",
  "license": "GPL-2.0-or-later",  
  "homepage": "https://github.com/Code-Klouds/wp-orm/",
  "support": {
    "issues": "https://github.com/Code-Klouds/wp-orm/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/database": "^8.0",
    "robmorgan/phinx": "^0.12.5"
  },
  "suggest": {
    "illuminate/events": "Add events to your models"
  },
  "autoload": {
    "files": [
      "src/includes/migrations.php"
    ],
    "psr-4": {
      "Dbout\\WpOrm\\": "src/"
    }
  }
}
